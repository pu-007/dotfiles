{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.37/schema.bar.json",
  "monitor": 0,
  "font_family": "JetXW",
  "theme": {
    "palette": "Catppuccin",
    "name": "Latte"
  },
  "height": 40,
  "transparency_alpha": 192,
  "left_widgets": [
    {
      "Media": {
        "enable": true
      }
    },
    {
      "Komorebi": {
        "workspaces": {
          "enable": true,
          "hide_empty_workspaces": true
        }
      }
    }
  ],
  "center_widgets": [
    {
      "Komorebi": {
        "layout": {
          "enable": true
        },
        "focused_window": {
          "enable": true,
          "show_icon": true
        }
      }
    }
  ],
  "right_widgets": [
    {
      "Update": {
        "enable": true
      }
    },
    {
      "Storage": {
        "enable": true
      }
    },
    {
      "Memory": {
        "enable": true,
        "label_prefix": "Icon"
      }
    },
    {
      "Cpu": {
        "enable": true,
        "label_prefix": "Icon"
      }
    },
    {
      "Network": {
        "enable": true,
        "show_default_interface": false,
        "show_activity": true,
        "show_total_activity": false
      }
    }
  ]
}
